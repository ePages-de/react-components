{
  "name": "@epages/react-components",
  "description": "A zoo of useful React components.",
  "version": "0.2.0",
  "scripts": {
    "start": "node ./webpack.devserver.js",
    "test": "jest",
    "build": "sh -c \"rm -rf build/ && NODE_ENV=production babel src/ --out-dir build/\"",
    "lint": "import-sort src/ && import-sort test/ && import-sort public/ && eslint --ext js,jsx *.js src/ test/ public/",
    "lint-fix": "import-sort -o src/; import-sort -o test/; import-sort -o public/; eslint --ext js,jsx *.js src/ test/ public/ --fix",
    "karma": "sh -c \"NODE_ENV=test karma start\"",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ePages-de/react-components.git"
  },
  "bugs": {
    "url": "https://github.com/ePages-de/react-components/issues"
  },
  "homepage": "https://github.com/ePages-de/react-components#readme",
  "license": "MIT",
  "main": "build/index.js",
  "peerDependencies": {
    "classnames": "^2.2.5",
    "color": "1.0.3",
    "hoist-non-react-statics": "^1.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.2.1",
    "immutable": "^3.8.1"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-es2015-modules-umd": "6.24.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-1": "6.24.1",
    "bluebird": "3.5.0",
    "classnames": "2.2.5",
    "color": "1.0.3",
    "eslint": "4.0.0",
    "eslint-config-standard": "10.2.1",
    "eslint-config-standard-react": "5.0.0",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-node": "5.0.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-react": "7.0.1",
    "eslint-plugin-standard": "3.0.1",
    "file-loader": "0.11.2",
    "glob": "7.1.2",
    "hoist-non-react-statics": "1.2.0",
    "immutable": "3.8.1",
    "import-sort-cli": "2.4.0",
    "import-sort-parser-babylon": "2.3.0",
    "import-sort-style-choffmeister": "1.0.0",
    "jest": "20.0.4",
    "json-loader": "0.5.4",
    "prop-types": "15.5.10",
    "react": "15.5.4",
    "react-addons-test-utils": "15.5.1",
    "react-dom": "15.5.4",
    "react-test-renderer": "15.5.4",
    "react-testutils-additions": "15.2.0",
    "sinon": "2.3.4",
    "unexpected": "10.29.0",
    "unexpected-immutable": "0.1.0",
    "unexpected-react": "4.1.0",
    "unexpected-sinon": "10.7.1",
    "webpack": "2.6.1",
    "webpack-dev-server": "2.4.5"
  },
  "importSort": {
    ".js, .jsx": {
      "parser": "babylon",
      "style": "choffmeister"
    }
  },
  "jest": {
    "setupFiles": [
      "./test/setup.js"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/",
      "<rootDir>/test/"
    ],
    "cacheDirectory": "<rootDir>/.cache/jest"
  },
  "false": {}
}
